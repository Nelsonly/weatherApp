<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SuperCircleView">

        <!-- 圆的半价 -->
        <attr name="min_circle_radio" format="integer"></attr>

        <!-- 底层默认圆环的进度条宽度-->
        <attr name="first_nomal_ring_width" format="float"></attr>

        <!-- 第二层默认圆环的进度条宽度-->
        <attr name="second_nomal_ring_width" format="float"></attr>

        <!-- 进度圆环的进度条宽度-->
        <attr name="color_ring_width" format="float"></attr>

        <!-- 圆环的宽度 -->
        <attr name="ring_width" format="float"></attr>

        <!-- 圆的颜色 -->
        <attr name="circle_color" format="color"></attr>

        <!-- 圆的颜色 -->
        <attr name="max_circle_color" format="color"></attr>

        <!-- 底层圆环的默认颜色 -->
        <attr name="ring_first_normal_color" format="color"></attr>

        <!-- 第二层圆环的默认颜色 -->
        <attr name="ring_sencond_normal_color" format="color"></attr>

        <!-- 圆环要显示的彩色的区域-->
        <attr name="ring_color_select" format="integer"></attr>
        <!-- 进度圆环的颜色-->
        <attr name="progress_circle_color" format="color"></attr>

        <!-- 第一行文本的内容-->
        <attr name="first_text" format="string"></attr>
        <!-- 第一行文本的大小-->
        <attr name="first_text_size" format="float"></attr>
        <!-- 第一行文本的颜色-->
        <attr name="first_text_color" format="color"></attr>

        <!-- 第二行文本的内容-->
        <attr name="second_text" format="string"></attr>
        <!-- 第二行文本的大小-->
        <attr name="second_text_size" format="float"></attr>
        <!-- 第二行文本的颜色-->
        <attr name="second_text_color" format="color"></attr>

        <!-- 第三行文本的内容-->
        <attr name="third_text" format="string"></attr>
        <!-- 第三行文本的大小-->
        <attr name="third_text_size" format="float"></attr>
        <!-- 第三行文本的颜色-->
        <attr name="third_text_color" format="color"></attr>

        <attr name="fontFamily" format="string" />

    </declare-styleable>

</resources>